{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "--dart-define=DEFINE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY"
      ]
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": [
        "--flavor",
        "staging",
        "--target",
        "lib/main_staging.dart",
        "--dart-define=DEFINE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY"
      ]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": [
        "--flavor",
        "production",
        "--target",
        "lib/main_production.dart",
        "--dart-define=DEFINE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY"
      ]
    }
  ]
}
